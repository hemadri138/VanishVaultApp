{
  "expo": {
    "name": "VanishVault",
    "slug": "vanishvault-expo",
    "scheme": "vanishvault",
    "version": "1.0.0",
    "icon": "./assets/vanishvault.png",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/vanishvault.png",
      "resizeMode": "contain",
      "backgroundColor": "#130f36"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vanishvault.png",
        "backgroundColor": "#130f36"
      },
      "package": "com.hemadri1316.vanishvaultexpo"
    },
    "web": {
      "favicon": "./assets/vanishvault.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
